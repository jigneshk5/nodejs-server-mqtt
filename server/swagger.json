{
    "openapi": "3.0.0",
    "info": {
      "description": "This is the documentation for our Mqtt server api",
      "version": "1.0.0",
      "title": "OBD Server API"
    },
    "host": "localhost:2000",
    "produces": ["application/json"],
    "schemes": [
      "http"
    ],
    "paths":{
        "/api/registration": {
            "get": {
              "description": "Returns all users registred with our System",
              "produces": [
                "application/json"
              ],
              "responses": {
                "200": {
                  "description": "Successfully got users",
                  "schema": {
                    "type": "array",
                    "items": [{}]
                  }
                }
              },
              "parameters": [
                {
                  "in": "query",
                  "name": "name",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by name"
                },
                {
                  "in": "query",
                  "name": "phone",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by phone number"
                },
                {
                  "in": "query",
                  "name": "mail",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by mail"
                },
                {
                  "in": "query",
                  "name": "rfid",
                  "schema": {
                    "type": "integer"
                  },
                  "required":false,
                  "description": "Filter by rfid"
                },
                {
                  "in": "query",
                  "name": "address",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by address"
                },
                {
                  "in": "query",
                  "name": "vin",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by vin"
                },
                {
                  "in": "query",
                  "name": "mac",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by mac"
                },
                {
                  "in": "query",
                  "name": "topic",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by topic"
                },
                {
                  "in": "query",
                  "name": "active",
                  "schema": {
                    "type": "integer"
                  },
                  "required":false,
                  "description": "Filter by active state"
                },
                {
                  "in": "query",
                  "name": "vt",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by vehicle type"
                },
                {
                  "in": "query",
                  "name": "ft",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by fuel type"
                },
                {
                  "in": "query",
                  "name": "fl",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by fuel level"
                },
                {
                  "in": "query",
                  "name": "fc",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by fuel capacity"
                },
                {
                  "in": "query",
                  "name": "milage",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by milage"
                },
                {
                  "in": "query",
                  "name": "du",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by date of updation"
                },
                {
                  "in": "query",
                  "name": "dc",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by date of creation"
                },
                {
                  "in": "query",
                  "name": "con",
                  "schema": {
                    "type": "string"
                  },
                  "required":false,
                  "description": "Filter by company name"
                }
              ]
            },
            "post": {
              "description": "Post a user to the DB",
              "produces": [
                "application/json"
              ],
              "requestBody":{
                "description": "Enter user details",
                "content": {
                    "application/x-www-form-urlencoded":
                    {
                        "schema": {
                            "type": "object",
                            "properties": {
                              "CustomerName": {
                                "type": "string"
                              },
                              "PHONE":{
                                "type": "string"
                              },
                              "EMAIL":{
                                "type": "string"
                              },
                              "ADDRESS":{
                                "type": "string"
                              },
                              "RFID":{
                                "type": "integer"
                              },
                              "VIN":{
                                "type": "string"
                              },
                              "MAC":{
                                "type": "string"
                              },
                              "topic":{
                                "type": "string"
                              },
                              "ACTIVE":{
                                "type": "integer"
                              },
                              "VEHICLE_TYPE":{
                                "type": "string"
                              },
                              "FUEL_TYPE":{
                                "type": "string"
                              },
                              "FUEL_LEVEL":{
                                "type": "string"
                              },
                              "FUEL_CAPACITY":{
                                "type": "string"
                              },
                              "MILAGE":{
                                "type": "string"
                              },
                              "DateOfCreation":{
                                "type": "string"
                              },
                              "DateOfUpdation":{
                                "type": "string"
                              },
                              "OtherData":{
                                "type": "string"
                              },
                              "CompanyName":{
                                "type": "string"
                              }
                            },
                            "required": [
                              "CustomerName","PHONE","EMAIL","ADDRESS","RFID","VIN","MAC","topic","ACTIVE",
                              "VEHICLE_TYPE","FUEL_TYPE","FUEL_LEVEL","FUEL_CAPACITY","MILAGE","DateOfCreation",
                              "DateOfUpdation","OtherData","CompanyName"
                            ]
                          }
                        }
                      }
                  },
              "responses": {
                "201": {
                  "description": "Successfully added the user",
                  "schema": {
                    "type": "array",
                    "items": [{}]
                  }
                }
              }
            }
          },
          "/{userId}": {
            "get": {
              "description": "Get a user registred by passing the userId",
              "produces": [
                "application/json"
              ],
              "parameters": [
                {
                    "name": "userId",
                    "in": "path",
                    "description": "ID of the user",
                    "required": true,
                    "schema": {
                      "type": "integer"
                    }
                  }
              ],
              "responses": {
                "200": {
                  "description": "Successfully got the user",
                  "schema": {
                    "type": "array",
                    "items": [{}]
                  }
                }
              }
            }
          },
          "/edit/{userId}": {
            "put": {
              "description": "Edit user details",
              "consumes": [
                "application/json"
              ],
              "produces": [
                "application/json"
              ],
              "parameters": [
                {
                    "name": "userId",
                    "in": "path",
                    "description": "ID of the user",
                    "required": true,
                    "schema": {
                      "type": "integer"
                    }
                  }
              ],
              "requestBody":{
                "description": "Edit user details",
                "content": {
                    "application/x-www-form-urlencoded":
                    {
                        "schema": {
                            "type": "object",
                            "properties": {
                              "CustomerName": {
                                "type": "string"
                              },
                              "PHONE":{
                                "type": "string"
                              },
                              "EMAIL":{
                                "type": "string"
                              },
                              "ADDRESS":{
                                "type": "string"
                              },
                              "RFID":{
                                "type": "integer"
                              },
                              "VIN":{
                                "type": "string"
                              },
                              "MAC":{
                                "type": "string"
                              },
                              "topic":{
                                "type": "string"
                              },
                              "ACTIVE":{
                                "type": "integer"
                              },
                              "VEHICLE_TYPE":{
                                "type": "string"
                              },
                              "FUEL_TYPE":{
                                "type": "string"
                              },
                              "FUEL_LEVEL":{
                                "type": "string"
                              },
                              "FUEL_CAPACITY":{
                                "type": "string"
                              },
                              "MILAGE":{
                                "type": "string"
                              },
                              "DateOfCreation":{
                                "type": "string"
                              },
                              "DateOfUpdation":{
                                "type": "string"
                              },
                              "OtherData":{
                                "type": "string"
                              },
                              "CompanyName":{
                                "type": "string"
                                }
                              }
                            }
                          }
                        }
                      },
              "responses": {
                "200": {
                  "description": "Successfully edited the user",
                  "schema": {
                    "type": "array",
                    "items": [{}]
                  }
                }
              }
            }
          }
        
    }
  }